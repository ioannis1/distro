{
  "service": {
                 "ID"     : "grok-{{ ansible_hostname }}",
                 "name"   : "grok-exporter",
                 "address": "localhost",
                 "port"   : 9100,
                 "token"  : "root",
                 "enable_tag_override" : false,
                 "tags"  : [ "exporter", "prometheus" ],
                 "check" : {
                             "name"     : "grok-exporter_check",
                             "tcp"      : "localhost:9144",
                             "timeout"  : "2s",
                             "interval" : "30s"
                          }
  }
}
