              "rawSql": " WITH\nnp AS (\n    SELECT\n        round(extract('epoch' from now() - stats_reset)/60)::numeric    AS min_since_reset\n         , (1024*1024 / 8192::numeric)                                  AS mp\n    FROM pg_stat_bgwriter                                               AS bg\n),\nbgstats AS (\n    SELECT  bg.stats_reset, bg.checkpoints_req, bg.checkpoint_write_time, checkpoint_sync_time\n            , buffers_checkpoint\n            , checkpoints_timed + checkpoints_req                         AS  total_checkpoints\n    FROM pg_stat_bgwriter bg, np\n),\nresult AS (\n    SELECT\n          round(100.0*checkpoints_req/total_checkpoints,1)                    AS  \"Forced checkpoint ratio (%)\"\n        , round(np.min_since_reset/total_checkpoints,2)                       AS  \"Minutes between checkpoints\"\n        , round(checkpoint_write_time::numeric/(total_checkpoints*1000),2)    AS  \"Average write time per checkpoint (s)\"\n        , round(checkpoint_sync_time::numeric/(total_checkpoints*1000),2)     AS  \"Average sync time per checkpoint (s)\"\n        , round(buffers_checkpoint/(np.mp*total_checkpoints),2)               AS  \"MB per checkpoint\"\n        , round(buffers_checkpoint/(np.mp*np.min_since_reset*60),2)           AS  \"Checkpoint (MBps)\"\n    FROM bgstats, np\n)\n\nSELECT now() AS time, *\nFROM result\n",
              "refId": "A"
            }
